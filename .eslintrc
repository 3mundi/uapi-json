{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "prefer-template": 0,
    "import/no-extraneous-dependencies": ["error",
      {"devDependencies": ["**/*.test.js", "**/*.spec.js"]}
    ]
  }
}
